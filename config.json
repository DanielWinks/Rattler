{
  "name": "Rattler 433",
  "version": "1.0.23",
  "slug": "rtl_433_hassio",
  "description": "RTL_433 for Home Assistant",
  "arch": [
    "armhf",
    "armv7",
    "aarch64",
    "amd64",
    "i386"
  ],
  "startup": "application",
  "boot": "auto",
  "services": [
    "mqtt:need"
  ],
  "map": [
    "share"
  ],
  "usb": true,
  "options": {
    "mqtt_prefix": "rtl_433",
    "retain": true,
    "customize": {
      "active": false,
      "folder": "rattler"
    }
  },
  "schema": {
    "mqtt_prefix": "str",
    "retain": "bool",
    "customize": {
      "active": "bool",
      "folder": "str?"
    }
  }
}